'''
for a in range(5000, 0, -1):
    flag = 0
    for x in range(1, 20000):
        if (((x % 40 == 0) or (x % 64 == 0)) <= (x % a == 0)) == 0:
            flag = 1
            break
    if flag == 0:
        print(a)
'''


'''
# функция
def f(x, a):    
    return ((x % 40 == 0) or (x % 64 == 0)) <= (x % a == 0)

for a in range(1, 1000):    
    if all(f(x,a) == 1 for x in range(1,1000)):
        print(a)
'''
